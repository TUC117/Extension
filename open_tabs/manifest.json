{
    "manifest_version": 3,
    "name": "Open Tabs",
    "description": "Opens particular Tabs when clicked on it",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html",
                "default_icon": {
                    "16": "/icons/icon-16.png",
                    "48": "/icons/icon-48.png",
                    "128": "/icons/icon-128.png"
                }
    },
    "permissions": [
        "tabs"
    ]
  }